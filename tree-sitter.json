{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "axle",
      "camelcase": "Axle",
      "title": "Axle",
      "scope": "source.axle",
      "file-types": [
        "axle",
        "axl"
      ],
      "injection-regex": "^axle$",
      "class-name": "TreeSitterAxle"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Axle grammar for tree-sitter",
    "authors": [
      {
        "name": "Thomas Hickson",
        "email": "veeloxfire@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/Veeloxfire/tree-sitter-axle"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}